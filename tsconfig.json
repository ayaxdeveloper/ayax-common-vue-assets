{
  "compilerOptions": {
    "outDir": "./built/",
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "emitDecoratorMetadata": true,
    "strictNullChecks": false,
    "lib": [ "es2015", "dom" ],
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}